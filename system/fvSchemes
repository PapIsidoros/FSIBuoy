FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
// Changed to Euler from Crank-Nicholson 0.5
    default         Euler;
}

gradSchemes
{
    default         Gauss linear;
}

divSchemes
{
// Changed to upwind from vanLeerV
    div(rhoPhi,U)  Gauss upwind;
// Changed to upwind from vanLeer
    div(phi,alpha)  Gauss upwind;
// Changed to interfaceCompression from linear
    div(phirb,alpha) Gauss interfaceCompression;
    div(phi,k)      Gauss upwind;
    div(phi,epsilon) Gauss upwind;
    div(((rho*nuEff)*dev2(T(grad(U))))) Gauss linear;
}

laplacianSchemes
{
    default         Gauss linear corrected;
}

interpolationSchemes
{
    default         linear;
}

snGradSchemes
{
    default         corrected;
}


// ************************************************************************* //
